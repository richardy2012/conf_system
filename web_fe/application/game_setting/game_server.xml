<?xml version="1.0" encoding="utf-8"?>
<game_server>
  <general>
    <service_id>[replace_game_server_host_id]</service_id>
    <io_thread_size>1</io_thread_size>
    <logpath>/data/logs/</logpath>
    <language>En</language>
  </general>
  <server>
    <network>[replace_game_server_network]</network>
    <ip>[replace_game_server_ip]</ip>
    <port>[replace_game_server_port]</port>
    <accept_queue_size>250</accept_queue_size>
    <io_thread_size>4</io_thread_size>
    <work_thread_init>4</work_thread_init>
    <work_thread_high>16</work_thread_high>
    <work_thread_load>500</work_thread_load>
    <handler_pool_init>100</handler_pool_init>
    <handler_pool_low>0</handler_pool_low>
    <handler_pool_high>1000</handler_pool_high>
    <handler_pool_inc>100</handler_pool_inc>
    <handler_pool_max>5000</handler_pool_max>
    <read_buffer_size>10240</read_buffer_size>
    <write_buffer_size>10240</write_buffer_size>
    <session_timeout>0</session_timeout>
    <io_timeout>60</io_timeout>
  </server>
  <load_balancer>
    <ip>[replace_load_balancer_ip]</ip>
    <port>[replace_load_balancer_port]</port>
    <read_buffer_size>256</read_buffer_size>
    <write_buffer_size>256</write_buffer_size>
    <session_timeout>0</session_timeout>
    <io_timeout>60</io_timeout>
    <retry_timeout>30</retry_timeout>
    <keepalive_timeout>30</keepalive_timeout>
  </load_balancer>
  <battle_balancer>
    <ip>[replace_battle_balancer_ip]</ip>
    <port>[replace_battle_balancer_port]</port>
    <io_thread_size>1</io_thread_size>
    <handler_pool_init>10</handler_pool_init>
    <handler_pool_low>0</handler_pool_low>
    <handler_pool_high>50</handler_pool_high>
    <handler_pool_inc>10</handler_pool_inc>
    <handler_pool_max>100</handler_pool_max>
    <buffer_size>256</buffer_size>
    <timeout_milliseconds>3000</timeout_milliseconds>
  </battle_balancer>
  <battle_client>
    <io_thread_size>4</io_thread_size>
    <work_thread_init>4</work_thread_init>
    <work_thread_high>16</work_thread_high>
    <work_thread_load>500</work_thread_load>
    <handler_pool_init>100</handler_pool_init>
    <handler_pool_low>0</handler_pool_low>
    <handler_pool_high>1000</handler_pool_high>
    <handler_pool_inc>100</handler_pool_inc>
    <handler_pool_max>5000</handler_pool_max>
    <read_buffer_size>10240</read_buffer_size>
    <write_buffer_size>10240</write_buffer_size>
    <session_timeout>0</session_timeout>
    <io_timeout>60</io_timeout>
  </battle_client>
  <php_game_service>
    <type>http</type>
    <address>http://PHP-SERVER:8784/main.php?stream=</address>
    <port>9090</port>
    <connect_timeout>10</connect_timeout>
    <receive_timeout>10</receive_timeout>
    <send_timeout>10</send_timeout>
    <read_buffer_size>10240</read_buffer_size>
    <write_buffer_size>10240</write_buffer_size>
    <pool_init>4</pool_init>
    <pool_low>0</pool_low>
    <pool_high>8</pool_high>
    <pool_inc>2</pool_inc>
    <pool_max>16</pool_max>
  </php_game_service>
  <php_cross_service>
    <type>http</type>
    <address>http://PHP-SERVER:8784/cross.php?stream=</address>
    <port>9090</port>
    <connect_timeout>10</connect_timeout>
    <receive_timeout>10</receive_timeout>
    <send_timeout>10</send_timeout>
    <read_buffer_size>10240</read_buffer_size>
    <write_buffer_size>10240</write_buffer_size>
    <pool_init>4</pool_init>
    <pool_low>0</pool_low>
    <pool_high>8</pool_high>
    <pool_inc>2</pool_inc>
    <pool_max>16</pool_max>
  </php_cross_service>
  <cpp_service>
    <ip>[replace_cpp_service_ip]</ip>
    <port>[replace_cpp_service_port]</port>
    <accept_timeout>10000</accept_timeout>
    <receive_timeout>10000</receive_timeout>
    <send_timeout>10000</send_timeout>
    <retry_limit>3</retry_limit>
    <retry_delay>3000</retry_delay>
    <tcp_send_buffer></tcp_send_buffer>
    <tcp_recv_buffer></tcp_recv_buffer>
  </cpp_service>
  <monitor>
    <ip>127.0.0.1</ip>
    <port>9010</port>
    <read_buffer_size>1024</read_buffer_size>
    <write_buffer_size>1024</write_buffer_size>
    <session_timeout>0</session_timeout>
    <io_timeout>60</io_timeout>
    <retry_timeout>30</retry_timeout>
    <keepalive_timeout>30</keepalive_timeout>
  </monitor>
  <redis>
    <sys_cache>
      <ip>REDIS-SYSDB-SERVER</ip>
      <port>6379</port>
    </sys_cache>
  </redis>
</game_server>